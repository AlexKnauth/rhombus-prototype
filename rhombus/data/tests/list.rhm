#lang rhombus


import:
  rhombus/data/list.List


check:
  List().size
  ~is 0

check:
  List("foo").size
  ~is 1

check:
  List("foo", "bar", "baz").size
  ~is 3

check:
  List("foo").get(0)
  ~is "foo"

check:
  List("foo", "bar", "baz").get(0)
  ~is "foo"

check:
  List("foo", "bar", "baz").get(1)
  ~is "bar"

check:
  List("foo", "bar", "baz").get(2)
  ~is "baz"

check:
  List().isEmpty()
  ~is #true

check:
  List(1, 2, 3).contains(2)
  ~is #true

check:
  List(1, 2, 3).contains(4)
  ~is #false

check:
  List.builder().build().size
  ~is 0
