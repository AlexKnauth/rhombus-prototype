#lang rhombus/and_meta
import:
  lib("racket/base.rkt").atan

class Posn(x, y)

annot.only.macro 'AlsoPosn': values('Posn', '')
check:
  Posn(1, 2) :: AlsoPosn
  ~is Posn(1, 2)

check:
  "no" :: AlsoPosn
  ~raises "value does not satisfy annotation"

bind.only.macro 'AlsoPosn ($x, $y) $tail ...':
  values('Posn($x, $y)', '$tail ...')

def AlsoPosn(also_x, also_y) = Posn(10, 20)
check:
  also_x +& "," +& also_y
  ~is "10,20"
