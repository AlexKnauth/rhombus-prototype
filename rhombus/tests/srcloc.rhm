#lang rhombus

block:
  import "static_arity.rhm"
  static_arity.check:
    Srcloc(f, l, c, p, s)
    Srcloc.to_report_string(srcloc)

check:
  use_static
  def s = Srcloc("here", 1, 2, 3, 4)
  [s.source, s.line, s.column, s.position, s.span]
  ~is ["here", 1, 2, 3, 4]

check:
  def s = dynamic(Srcloc("here", 1, 2, 3, 4))
  [s.source, s.line, s.column, s.position, s.span]
  ~is ["here", 1, 2, 3, 4]
